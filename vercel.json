{
  "env": {
    "SERVERLESS": "True",
    "WEBHOOK_HOST": "https://tg-serverless.vercel.app",
    "BOT_API_TOKEN": ""
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    },
    {
      "src": "/api",
      "dest": "/",
      "status": 308
    },
    {
      "src": "/api/((?!bot).*)",
      "dest": "/",
      "status": 308
    },
    {
      "src": "/github",
      "dest": "https://github.com/illvart/tg-serverless",
      "status": 308
    }
  ]
}
